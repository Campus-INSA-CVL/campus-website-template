{
  "name": "campus-website-templates",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "sandbox": "npm i --production=false && npm run dev",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
   "lint:md": "eslint --ext .md content/",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:md -- --fix",
    "lint": "npm run lint:js && npm run lint:md"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint:js",
    "content/**": "npm run lint:md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@mdi/js": "^5.5.55",
    "@nuxt/content": "^1.7.0",
    "base64toblob": "0.0.2",
    "file-saver": "^2.0.2",
    "jszip": "^3.5.0",
    "nuxt": "^2.14.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/tailwindcss": "^2.1.1",
    "@nuxtjs/vuetify": "^1.11.2",
    "@tailwindcss/typography": "^0.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-preset-lint-markdown-style-guide": "^3.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "sass-loader": "^7.3.1"
  }
}
